{
  "name": "typing-race-app",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "server",
    "client"
  ],
  "scripts": {
    "install-all": "npm install --workspaces",
    "dev:server": "npm run dev --workspace server",
    "dev:client": "npm run dev --workspace client",
    "dev": "npm-run-all --parallel dev:server dev:client"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
